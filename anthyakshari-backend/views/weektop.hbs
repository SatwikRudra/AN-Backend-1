  <title>Document</title>
</head>
<body>
<nav class="navbar navbar-expand-lg navbar-light bg-light" style="   background: #563d7c !important;">
  <a class="navbar-brand" href="#">Navbar</a>
  <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
    <span class="navbar-toggler-icon"></span>
  </button>
  <div class="collapse navbar-collapse" id="navbarNav">
    <ul class="navbar-nav ml-auto" >
      <li class="nav-item active">
        <a class="nav-link" href="/" style="color:#fff !important;">Home <span class="sr-only">(current)</span></a>
      </li>

  {{#if user}}
      <li class="nav-item">
        <a class="nav-link" href="/logout" style="color:#fff !important;">Logout</a>
      </li>
      <li class="nav-item">
        <a class="nav-link" href="#" style="color:#fff !important;">Profile</a>
      </li>
{{else}}
  <li class="nav-item">
        <a class="nav-link" href="/login" style="color:#fff !important;">Login</a>
      </li>
      <li class="nav-item">
        <a class="nav-link" href="/register" style="color:#fff !important;">Register</a>
      </li>

{{/if}}

    </ul>
  </div>
</nav>
  <div class="container">
<form action="/auth/weektop" method="post">
<button type="submit" class="btn btn-warning">You Won</button>
</form>
<h1>Weekly Top Performer</h1>
      <table class="table">
  <thead>
    <tr>
  
      <th scope="col">user_Id</th>
      <th scope="col">Position</th>
      <th scope="col">Name</th>
      <th scope="col">Coins</th>
    </tr>
  </thead>
  <tbody>
        {{#each data}}
            <tr scope="row">
                <td>{{this.user_Id}}</td>
                <td>{{this.livecmp_Count}}</td>
                <td>{{this.userName}}</td>
                <td>{{this.userCoins}}</td>
            </tr>
        {{/each}}
   
  
   
  </tbody>
</table>
    </div>
</body>
</html>